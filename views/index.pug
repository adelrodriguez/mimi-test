extends layout.pug
block styles
  link(
    rel='stylesheet'
    type='text/css'
    href='/css/typeform.css'
  )
block content
  iframe(
    id='typeform-full'
    width='100%'
    height='100%'
    frameborder='0'
    allow='camera; microphone; autoplay; encrypted-media;'
    src= url
  )
block scripts
  script(
    type='text/javascript'
    src='https://embed.typeform.com/embed.js'
  )
